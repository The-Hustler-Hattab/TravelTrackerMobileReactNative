{
  "expo": {
    "name": "travel-tracker",
    "slug": "travel-tracker",
    "scheme": "travel-tracker",

    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
